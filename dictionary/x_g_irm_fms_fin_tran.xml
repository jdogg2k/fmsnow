<?xml version="1.0" encoding="UTF-8"?><database>
    <element label="Financial Transaction" max_length="40" name="x_g_irm_fms_fin_tran" type="collection">
        <element label="Account Number" max_length="4" name="account_nmbr" type="string"/>
        <element label="Allocation Group" max_length="32" name="allocation_group_ref" reference="x_g_irm_fms_allocation_group" type="reference"/>
        <element label="Allotment" mandatory="true" max_length="32" name="allotment_ref" reference="x_g_irm_fms_allotment" type="reference"/>
        <element label="Contract" max_length="32" name="contract_ref" reference="x_g_irm_fms_contract" reference_qual="javascript: &quot;organization_refIN&quot; + new OrgHierarchy().GetParentOrgs(current.project_ref.organization_ref);" type="reference" use_reference_qualifier="advanced"/>
        <element label="Cost Pool" max_length="32" name="cost_pool_ref" reference="x_g_irm_fms_cost_pool" type="reference"/>
        <element label="Description" max_length="50" name="description" type="string"/>
        <element calculation="(function calculatedFieldValue(current) {&#13;&#10;&#9;var display;&#13;&#10;&#9;if (current.sys_id.nil()) {&#13;&#10;&#9;&#9;display = &quot;[new financial transaction]&quot;;&#13;&#10;&#9;}&#13;&#10;&#9;else {&#13;&#10;&#9;&#9;display = current.description.toString() + '\t\t\t\t[' + current.tran_type.toString() + '-' + current.tran_status + ']'; &#13;&#10;&#9;}&#13;&#10;&#9;return display;&#13;&#10;&#13;&#10;})(current);" display="true" label="Fin Tran display" max_length="256" name="fin_tran_display" read_only="true" type="string" virtual="true"/>
        <element choice="3" choice_field="fy" choice_table="x_g_irm_fms_project_allocation" default="javascript:new Date().getFullYear();" label="FY" mandatory="true" max_length="40" name="fy" type="integer">
            <choice/>
        </element>
        <element label="Notes" max_length="500" name="notes" type="string"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" type="string"/>
        <element label="Obligation" max_length="15" name="obligation" type="string"/>
        <element label="Obligation Date" max_length="40" name="obligation_date" type="glide_date"/>
        <element label="Appropriation" mandatory="true" max_length="32" name="project_appropriation_ref" reference="x_g_irm_fms_project_appropriation" type="reference"/>
        <element label="Project" mandatory="true" max_length="32" name="project_ref" reference="x_g_irm_fms_project" reference_qual="javascript: &quot;organization_refIN&quot; + new OrgHierarchy().GetUserOrgs();" type="reference" use_reference_qualifier="advanced"/>
        <element label="Received By" max_length="50" name="received_by" type="string"/>
        <element label="Reconciliation Date" max_length="40" name="reconcile_date" type="glide_date"/>
        <element calculation="(function calculatedFieldValue(current) {&#13;&#10;    var items = new GlideRecord('x_g_irm_fms_fin_tran_item');&#13;&#10;    items.addQuery('fin_tran_ref', current.sys_id);&#13;&#10;    items.query();&#13;&#10;    var total = 0;&#13;&#10;    while (items.next()) {&#13;&#10;        total += (parseInt(items.received_qty || items.item_qty)) * (parseFloat(items.actual_cost || items.item_cost));&#13;&#10;    }&#13;&#10;    return total; &#13;&#10;})(current);" label="Transaction Total" max_length="20" name="transaction_total_amt" read_only="true" type="currency" virtual="true"/>
        <element label="Transaction Date" mandatory="true" max_length="40" name="tran_date" type="glide_date"/>
        <element label="Status" mandatory="true" max_length="12" name="tran_status" type="string"/>
        <element label="Transaction Type" mandatory="true" max_length="16" name="tran_type" type="string"/>
        <element label="Void?" max_length="40" name="void" type="boolean"/>
        <index name="index">
            <element name="project_ref"/>
        </index>
        <index name="index2">
            <element name="allocation_group_ref"/>
        </index>
        <index name="index3">
            <element name="contract_ref"/>
        </index>
        <index name="index4">
            <element name="cost_pool_ref"/>
        </index>
        <index name="index5">
            <element name="project_appropriation_ref"/>
        </index>
        <index name="index6">
            <element name="allotment_ref"/>
        </index>
    </element>
</database>
