        <calculation><![CDATA[(function calculatedFieldValue(current) {
	if (current.pool_status != "Received") return 0;
    var items = new GlideRecord('x_g_irm_fms_allocation_pool');
    items.addQuery('pool_parent_ref', current.sys_id);
    items.query();
    var total = 0;
    while (items.next()) {
        total += parseFloat(items.receive_amt);
    }
    return total; 
})(current);]]></calculation>
