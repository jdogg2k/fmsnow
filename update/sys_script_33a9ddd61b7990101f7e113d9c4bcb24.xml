        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

    if (current.budget_type != previous.budget_type) {
        gs.addErrorMessage("Budget Type cannot be altered.  Copy or create a new budget.");
        current.setAbortAction(true);
    }
    if (current.fy != previous.fy) {
        gs.addErrorMessage("Budget Fiscal Year cannot be altered.  Copy or create a new budget.");
        current.setAbortAction(true);
    }

})(current, previous);]]></script>
