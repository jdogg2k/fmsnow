        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
    var newID = current.party_name;
    var contract = current.contract_ref;
    var uniqueCheck = new GlideRecord(FMSConstants.TABLE_CONTRACT_PARTY);
    uniqueCheck.addQuery("contract_ref", contract);
    uniqueCheck.addQuery("party_name", newID);
    uniqueCheck.query();
    if (uniqueCheck.next()) { //we have existing records with that ID, notify and cancel update
        gs.addErrorMessage("Contract Party " + newID + " is not unique. The same Contract Party can't be entered twice on one Contract as Interested Party.  Update Failed.");
        current.setAbortAction(true);
    }
})(current, previous);]]></script>
