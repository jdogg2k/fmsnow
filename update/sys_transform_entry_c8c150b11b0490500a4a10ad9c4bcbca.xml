        <source_script><![CDATA[answer = (function transformEntry(source) {

	var currencyOut = 0;
	
	var valStr = source.u_totalvalue;
	if (valStr != "") {
		valStr = valStr.replace("$", "");
		valStr = valStr.replace("M", "");
		var valNum = parseFloat(valStr);
		currencyOut = valNum * 1000000; //multiply times a million
	}
	

	return currencyOut; // return the value to be put into the target field
	
})(source);]]></source_script>
