        <source_script><![CDATA[answer = (function transformEntry(source) {

	var eCode = source.u_tran_id.toString();
	var codeLength = eCode.length;
	var ePrefix = "TRN0";
	if (codeLength == 1)
		ePrefix += "00000";
	if (codeLength == 2)
		ePrefix += "0000";
	if (codeLength == 3)
		ePrefix += "000";
	if (codeLength == 4)
		ePrefix += "00";
	if (codeLength == 5)
		ePrefix += "0";
	
	var transNumber = ePrefix + eCode;
	// Add your code here
	return transNumber; // return the value to be put into the target field

})(source);]]></source_script>
