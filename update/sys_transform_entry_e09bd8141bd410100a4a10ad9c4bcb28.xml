        <source_field>[Script]</source_field>
        <source_script><![CDATA[answer = (function transformEntry(source) {
	
	var targSys = "";
	var budgetCode = source.u_budget_code;
	var activityCode = source.u_activity_code;

	var gr = new GlideRecord("x_g_irm_fms_expense_item");
	gr.addEncodedQuery("budget_code_ref.budget_code=" + budgetCode + "^activity_code=" + activityCode);
	gr.query();
	if (gr.next())
		targSys = gr.getValue("sys_id");

	return targSys;

})(source);]]></source_script>
