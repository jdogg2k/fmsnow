        <calculation><![CDATA[(function calculatedFieldValue(current) {
    var items = new GlideRecord('x_g_irm_fms_fin_tran_item');
    items.addQuery('fin_tran_ref', current.sys_id);
    items.query();
    var total = 0;
    while (items.next()) {
        total += (parseInt(items.received_qty || items.item_qty)) * (parseFloat(items.actual_cost || items.item_cost));
    }
    return total; 
})(current);]]></calculation>
