        <audit>false</audit>
        <calculation><![CDATA[(function calculatedFieldValue(current) {
	var total = 0;
	
	if (current.budget_type != "") {
		var items = new GlideRecord('x_g_irm_fms_budget_item');
		items.addQuery('budget_ref', current.sys_id);
		items.query();
		while (items.next()) {
			total += parseFloat(items.budget_amt);
		}
	}
   
    return total; 
})(current);]]></calculation>
